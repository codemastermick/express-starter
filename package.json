{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.bundle.js",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack.config.js --no-watch",
    "build:watch": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "npm run build && concurrently --kill-others-on-fail \"nodemon --watch src -e ts,js --exec npm run start\" \"npm run build:watch\"",
    "start": "node --unhandled-rejections=strict ./dist/index.bundle.js",
    "start:debug": "cross-env DEBUG=* npm run start",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --unhandled-rejections=strict",
    "test:debug": "cross-env DEBUG=* npm test"
  },
  "keywords": [],
  "author": "Codemaster Mick",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.3",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "express-winston": "^4.2.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.14",
    "shortid": "^2.2.16",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
